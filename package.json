{
  "name": "angular-dsv",
  "version": "1.0.1",
  "description": "",
  "main": "lib/angular-dsv.js",
  "files": [
    "lib",
    "angular-dsv.js",
    "angular-dsv.min.js"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "d3-dsv": "^0.1.9"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "uglify-js": "^2.6.1",
    "xo": "^0.16.0"
  },
  "scripts": {
    "test": "npm run build && karma start",
    "uglify": "uglifyjs angular-dsv.js -o angular-dsv.min.js --mangle",
    "browserify": "browserify lib/angular-dsv.js -s angular-dsv -o angular-dsv.js",
    "build": "npm run browserify && npm run uglify",
    "lint": "semistandard"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Hypercubed/angular-dsv.git"
  },
  "keywords": [
    "angularjs",
    "dsv",
    "tsv",
    "csv",
    "tabular",
    "data"
  ],
  "author": "J. Harshbarger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hypercubed/angular-dsv/issues"
  },
  "homepage": "https://github.com/Hypercubed/angular-dsv",
  "xo": {
    "esnext": false,
    "space": 2,
    "envs": [
      "browser"
    ],
    "ignores": [
      "angular-dsv.*"
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "no-warning-comments": 0
    }
  }
}
